#: of pandapower.create.create_ext_grid:2
msgid "Creates an external grid connection."
msgstr ""

#: of pandapower.create.create_ext_grid:4
msgid ""
"External grids represent the higher level power grid connection and are "
"modelled as the slack bus in the power flow calculation."
msgstr ""

#: of pandapower.create.create_ext_grid:10
msgid "INPUT:"
msgstr ""

#: of pandapower.create.create_ext_grid:8
msgid "**net** - pandapower network"
msgstr ""

#: of pandapower.create.create_ext_grid:10
msgid "**bus** (int) - bus where the slack is connected"
msgstr ""

#: of pandapower.create.create_ext_grid:43
msgid "OPTIONAL:"
msgstr ""

#: of pandapower.create.create_ext_grid:13
msgid "**vm_pu** (float, default 1.0) - voltage at the slack node in per unit"
msgstr ""

#: of pandapower.create.create_ext_grid:15
msgid ""
"**va_degree** (float, default 0.) - voltage angle at the slack node in "
"degrees*"
msgstr ""

#: of pandapower.create.create_ext_grid:17
msgid "**name** (string, default None) - name of of the external grid"
msgstr ""

#: of pandapower.create.create_ext_grid:19
msgid "**in_service** (boolean) - True for in_service or False for out of service"
msgstr ""

#: of pandapower.create.create_ext_grid:21
msgid ""
"**s_sc_max_mva** (float, NaN) - maximal short circuit apparent power to "
"calculate internal             impedance of ext_grid for short circuit "
"calculations"
msgstr ""

#: of pandapower.create.create_ext_grid:23
msgid ""
"**s_sc_min_mva** (float, NaN) - minimal short circuit apparent power to "
"calculate internal             impedance of ext_grid for short circuit "
"calculations"
msgstr ""

#: of pandapower.create.create_ext_grid:25
msgid ""
"**rx_max** (float, NaN) - maximal R/X-ratio to calculate internal "
"impedance of ext_grid             for short circuit calculations"
msgstr ""

#: of pandapower.create.create_ext_grid:27
msgid ""
"**rx_min** (float, NaN) - minimal R/X-ratio to calculate internal "
"impedance of ext_grid             for short circuit calculations"
msgstr ""

#: of pandapower.create.create_ext_grid:29
msgid ""
"**max_p_mw** (float, NaN) - Maximum active power injection. Only "
"respected for OPF"
msgstr ""

#: of pandapower.create.create_ext_grid:31
msgid ""
"**min_p_mw** (float, NaN) - Minimum active power injection. Only "
"respected for OPF"
msgstr ""

#: of pandapower.create.create_ext_grid:33
msgid ""
"**max_q_mvar** (float, NaN) - Maximum reactive power injection. Only "
"respected for OPF"
msgstr ""

#: of pandapower.create.create_ext_grid:35
msgid ""
"**min_q_mvar** (float, NaN) - Minimum reactive power injection. Only "
"respected for OPF"
msgstr ""

#: of pandapower.create.create_ext_grid:37
msgid ""
"**r0x0_max** (float, NaN) - maximal R/X-ratio to calculate Zero sequence "
"internal impedance of ext_grid"
msgstr ""

#: of pandapower.create.create_ext_grid:40
msgid ""
"**x0x_max** (float, NaN) - maximal X0/X-ratio to calculate Zero sequence "
"internal impedance of ext_grid"
msgstr ""

#: of pandapower.create.create_ext_grid:43
msgid "** only considered in loadflow if calculate_voltage_angles = True"
msgstr ""

